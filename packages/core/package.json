{
  "name": "@a2a/core",
  "version": "0.1.0",
  "description": "Core utilities and abstractions for A2A agent system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "default": "./dist/agents/index.js"
    },
    "./vertex": {
      "types": "./dist/vertex/index.d.ts",
      "default": "./dist/vertex/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "default": "./dist/mcp/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.7.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "p-retry": "^6.2.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
